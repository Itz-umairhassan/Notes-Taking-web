{{>header}}

        <div id="notes-section">
            <div id="notes-container">
              {{#each NoteData}}
                <a href={{this._id}} class="link">
                    <div class="notes-item">
                        <div id="notes-data">
                            <p>{{this.noteData}}</p>

                        </div>
                        <div class="notes-title">
                            <h2>{{getShort this.title}}</h2>
                        </div>

                        <div class="notes-time">
                            <p>9 months ago</p>
                        </div>
                    </div>
                </a>
                {{/each}}
                
            </div>
        </div>

      
    </section>


    </body>

</html>
